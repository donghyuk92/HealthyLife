<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.example.com.midterm" >
    <uses-sdk android:minSdkVersion="16"></uses-sdk>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Alarm.AlarmSettingActivity"
            android:label="@string/title3"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity android:name=".Alarm.AlarmingActivity"
            android:label="@string/title3"
            android:parentActivityName=".MainActivity" >
        </activity>
        <service
            android:name=".Alarm.AlarmService">
        </service>
        <receiver
            android:name=".Alarm.Receiver">
        </receiver>
        <activity android:name=".Calorie.FirstActivity"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar"
            android:label="@string/calorie"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity android:name=".Calorie.SecondActivity"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar"
            android:label="@string/calorieinfo"
            android:parentActivityName=".Calorie.FirstActivity" >
        </activity>

        <activity
            android:name=".PhotoDiary.SetupActivity"
            android:label=" "
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity
            android:name=".PhotoDiary.ImageGridActivity"
            android:label=" "
            android:theme="@android:style/Theme.Holo.Dialog.NoActionBar" >
        </activity>
        <service android:name=".MyService" >
            <intent-filter>
                <action android:name="android.example.com.midterm.actionplay" />
                <action android:name="android.example.com.midterm.actionpause" />
                <action android:name="android.example.com.midterm.actionstop" />
                <action android:name="android.example.com.midterm.actionchange" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </service>
    </application>
</manifest>
